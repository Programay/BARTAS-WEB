{
  "extends": [
    "@vue/tsconfig/tsconfig.node.json",
    "node_modules"
  ],
  "include": [
    "vite.config.*",
    "vitest.config.*",
    "cypress.config.*",
    "playwright.config.*",
    "src/**/*.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "src/**/*.jsx"
  ],
  "compilerOptions": {
    "composite": true,
    "resolveJsonModule": true,
    "types": [
      "vite/client"
    ]
  }
}
